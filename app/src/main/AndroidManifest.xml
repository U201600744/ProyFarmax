<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.proyecto.proyfarmax">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProyFarmax"
        android:usesCleartextTraffic="true">
        <activity android:name=".view.CarritoCompras"></activity>
        <activity android:name=".view.ListaProductos">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.RegistrarUsuario_2" />
        <activity android:name=".view.RegistrarUsuario_1" />
        <activity android:name=".view.RecuperarPassword" />
        <activity android:name=".view.Login"></activity>
        <activity android:name=".view.RegistrarUsuario" />
        <activity
            android:name=".view.RegistrarActivity"
            android:label="REGISTRAR PRODUCTOS"
            android:parentActivityName=".view.MainActivity"></activity>
        <activity
            android:name=".view.EditarActivity"
            android:label="EDITAR PRODUCTOS"
            android:parentActivityName=".view.MainActivity" />
        <activity android:name=".view.MainActivity" />
    </application>

</manifest>