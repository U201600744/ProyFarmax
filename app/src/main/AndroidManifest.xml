<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.proyecto.proyfarmax">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProyFarmax"
        android:usesCleartextTraffic="true">
        <activity android:name=".views.RegistrarActivity" android:label="REGISTRAR PRODUCTOS" android:parentActivityName=".views.MainActivity"></activity>
        <activity android:name=".views.EditarActivity" android:label="EDITAR PRODUCTOS" android:parentActivityName=".views.MainActivity"></activity>

        <activity android:name=".views.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>



    </application>

</manifest>